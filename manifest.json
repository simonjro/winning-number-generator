{
    "manifest_version": 2,

    "name": "Winning Number Generator",
    "version": "1.0",
    "author": "@simonjro",
    "description": "Chrome extension to override the number generated by the Google random number generator widget.",
    "icons": {
        "16": "assets/img/icon16.png",
        "19": "assets/img/icon19.png",
        "38": "assets/img/icon38.png",
        "48": "assets/img/icon48.png",
        "128": "assets/img/icon128.png"
    },

    "content_scripts": [{
        "matches": ["https://www.google.com/*"],
        "js": ["content.js"],
        "run_at": "document_end"
    }],

    "browser_action": {
      "default_icon": "assets/img/icon128.png",
      "default_popup": "popup.html",
      "default_title": "Winning Number Generator"
    },

    "permissions": [
      "activeTab"
    ],

    "converted_from_user_script": true,
    "offline_enabled": true
}
